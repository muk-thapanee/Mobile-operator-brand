html, body {
  font-size: 16px;
}

body {
  background: #99ccff;
  /* fallback for old browsers */
  background: -webkit-linear-gradient(to bottom, #c473c4, #99ccff);
  /* Chrome 10-25, Safari 5.1-6 */
  background: linear-gradient(to bottom, #c473c4, #99ccff);
  /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */
  background-repeat: no-repeat;
  height: auto;
}
